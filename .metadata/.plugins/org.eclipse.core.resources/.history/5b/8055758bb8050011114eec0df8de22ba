package stream;

import java.util.Arrays;
import java.util.List;
import java.util.Spliterator;
import java.util.stream.Stream;

public class SpecialIterator {
	public static void main(String[] args) {
		
		List<String> names = Arrays.asList("Sanket","Shankar","Khokade");
		
		Spliterator<String> splititerator = names.stream().spliterator();
		splititerator.forEachRemaining(n -> System.out.println(splititerator));
		
		
		
	}

}
