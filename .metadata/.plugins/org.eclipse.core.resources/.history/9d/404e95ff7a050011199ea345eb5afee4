	Person p1 = new Person("Sanket", 22);
	Person p2 = n

}


class Person{
	
	String name;
	String country;
	
	@Override
	public String toString() {
		return "Person [name=" + name + ", country=" + country + "]";
	}

	public Person(String name, String country) {
		super();
		this.name = name;
		this.country = country;
	}	
}
